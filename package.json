{
  "name": "dyson-swarm",
  "version": "1.0.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "start": "vite preview",
    "validate": "svelte-check --tsconfig ./tsconfig.json --use-new-transformation",
    "test": "jest src",
    "test:watch": "npm run test -- --watch"
  },
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.10",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@threlte/core": "^5.0.9",
    "@threlte/preprocess": "^0.0.2",
    "@tsconfig/svelte": "^5.0.4",
    "@types/css-font-loading-module": "0.0.7",
    "@types/jest": "^27.5.2",
    "@types/three": "^0.149.0",
    "augmented-ui": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3",
    "jssm": "^5.88.3",
    "postcss": "^8.4.20",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.0",
    "svelte-jester": "^3.0.0",
    "svelte-preprocess": "^6.0.1",
    "svelte-sequential-preprocessor": "^0.0.7",
    "sql.js": "^1.10.3",
    "tailwindcss": "^3.4.4",
    "three": "^0.149.0",
    "ts-jest": "^28.0.8",
    "tslib": "^2.4.1",
    "typescript": "^5.4.5",
    "vite": "^5.3.1"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ]
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ]
  }
}
